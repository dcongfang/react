.tabbar
  overflow-x hidden
  position fixed
  bottom 0
  height 50px
  width 100%
  display flex
  z-index 19
  background #fff
  .tab-item
    flex 1
    text-align center
    p
      margin-top 2px
      font-size 12px

.nav-link, .link
  display block
  color inherit
  text-decoration none
  &.active
    background-color #f3f3f3
.translate-enter
  transform translate3d(100%,0,0)
  &.translate-enter-active
    transition transform .3s
    transform translate3d(0,0,0)

.App
  overflow-x hidden
  width 100%
  height 100%
  .music-view
    width 100%
    height 100%

.circle
  position: absolute;
  box-sizing: border-box;
  top: -0.1667rem;
  height: 2.4rem;
  min-width: 2.4rem;
  line-height: 1.6;
  text-align: center;
  padding: 0 0.783rem;
  border-radius: 1.2rem;
  background: #d44e45;
  font-size: 1.5rem;
  font-weight: 700;
  color: #fff;
  text-shadow: 0 -2px 0 rgba(0,0,0,0.06);
  transform: scale(0.33333);
  box-shadow: 0 3px 8px rgba(212,78,69,0.4);
  transition: transform 0.3s ease-out;
  margin-top: -9px;
  margin-left: 3px;