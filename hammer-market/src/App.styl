.tabbar
  overflow-x hidden
  position fixed
  bottom 0
  height 40px
  width 100%
  display flex
  z-index 19
  background #fff
  .tab-item
    flex 1
    text-align center

.nav-link, .link
  display block
  color inherit
  text-decoration none
  &.active
    background-color #e6e6e6
.translate-enter
  transform translate3d(100%,0,0)
  &.translate-enter-active
    transition transform .3s
    transform translate3d(0,0,0)

.App
  overflow-x hidden
  width 100%
  height 100%
  .music-view
    width 100%
    height 100%
