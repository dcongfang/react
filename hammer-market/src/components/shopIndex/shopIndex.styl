.header 
  overflow-x hidden
  height 50px
  width 100%
  z-index 99
  position relative
  display flex
  background-color #e6e6e6
  .icon-font
    width 10%
    height 20px
    margin-left 10px
    margin-top 15px
    margin-bottom 15px
  .icon-font1
    flex 1
    margin-top 10px
    margin-bottom 10px
    text-align center
  .icon-font2
    width 10%
    height 30px
    margin-top 10px
    margin-bottom 10px
    margin-right 10px

 .swiper2
    height: 150px
    padding-left 10px
    padding-right 10px
    z-index -100
    .swiper-slide
      height 100%
      width 100%
      img 
        border-radius 17px

.white
  width 100%
  height 40px
  background-color #fff

.bg-color
  width 100%
  height 10px
  background-color #e6e6e6

  &.active 
    transform translateX(px2rem(400px))
    opacity 1
    


      


  