.nav-title
    color #fff
    font-size 18px
    line-height 20px
    padding 16px 0 0
    text-align center
    position absolute
    top 0
    left 60px
    right 60px
    overflow hidden
    text-overflow ellipsis
    white-space nowrap

.main-wrapper
  margin-top 50px
  margin-top 75px
  margin-left 12.5px
  margin-right 12.5px
  height 700px
  .page-for-center
    background-color #fff
    height 147.5px
    width 100%
    border-radius 10px
    position: relative;
    box-shadow: 0 1px 2px rgba(0,0,0,.1), 0 18px 20px -10px rgba(0,0,0,.03), 0 18px 20px -10px rgba(0,0,0,.03), 0 10px 20px -10px rgba(0,0,0,.03);
    .wrapper
      background-color #fff
      border-radius 10px
      height 85px
      display flex
      width 100%
      position relative
      border-bottom 1px solid #f3f3f3
      .avatar
        height 55px
        width 55px
        margin-top: 10px;
        margin-left: 10px;
        text-align center
      .box-userinfo
        margin-left 10px
        text-align center
        margin-top 30px
        width 100px
        font-size 1rem
      .arrow-right-icon
        margin-left: 150px;
        margin-top: 25px;
        text-align: center;
        font-size: 14px;
    .menu-list-parallel
      margin-top 0
      margin-bottom 30px
      height 62.5px
      width 100%
      display flex
      .icon-order
        width 25%
        text-align center
        height 54.22px
        margin-bottom 0
        border-left: 1px solid #f3f3f3
        img
          height 21px
          width 21px
          text-align center
          margin-top 8px
        p
          margin-top 6px
          margin-bottom 0
          font-size 2px

.menu-list-vertical
  margin-top 30px
  border-radius 7px
  height 250px
  width 100%
  background-color #fff
  box-shadow: 0 1px 2px rgba(0,0,0,.1), 0 18px 20px -10px rgba(0,0,0,.03), 0 18px 20px -10px rgba(0,0,0,.03), 0 10px 20px -10px rgba(0,0,0,.03);
  margin-bottom 30px
  .arrow-right-icons
    display flex
    height 62.5px
    width 100%
    margin-bottom 0
    .text-con
      margin-left 10px
      width 100px
      font-size 16px
      line-height 65.5px
    .arrow-right-icon
      margin-left 215px
      line-height 65.5px
      font-size 14px

