.category-onethree-container
  display: flex;
  flex-wrap: wrap;
  padding-bottom: 0;
  padding-left 15px
  .category-onethree
    width: 30%;
    margin-right: .4rem;
    .skus-layout
      text-align: center;
      .image-container
        width: 100%;
        padding-top: 100%;
        border-radius: .2rem;
        position: relative;
        img
          position: absolute;
          left: 1rem;
          top: 1rem;
          width: calc(100% - 2rem);
          height: calc(100% - 2rem);
      .title
        width: 120%;
        transform: scale(.83333);
        transform-origin: 0 50%;
        margin: .3rem 0 .8rem;
        font-size: .5rem;
        font-weight: 700;
        line-height: 1.2;
        display: block;
        text-align: center;
        color: rgba(0,0,0,.8);
        overflow: hidden;
        word-break: break-word;
        text-overflow: ellipsis;
        white-space: nowrap;

.image-container:before {
    content: "";
    position: absolute;
    top: 0;
    bottom: -42.85714%;
    left: 0;
    right: -42.85714%;
    border: 1px solid #f0f0f0;
    -webkit-transform: scale(.7);
    transform: scale(.7);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
}

